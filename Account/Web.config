      </authorization>
    </system.web>
  </location>
  <location path="usuarioDetalle.aspx">
    <system.web>
      <authorization>
        <allow roles="ADMINISTRACION"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
</configuration>
